
@{
    ViewData["Title"] = "Register";
}

<div class="container-fluid">
    <div class="row">
        <div class="col"></div>
        <div class="col-4 d-flex justify-content-center" style="background-color:tan;">
            <form>
              <div class="form-group">
                <label for="FirstNameIn">First Name</label>
                <input type="text" class="form-control" id="FirstNameIn" placeholder="Enter First Name">
              </div>
              <div class="form-group">
                <label for="LastNameIn">Last Name</label>
                <input type="text" class="form-control" id="LastNameIn" placeholder="Enter Last Name">
              </div>
              <div class="form-group">
                <label for="DateOfBirthIn">Date of Birth</label>
                <input type="date" class="form-control" id="DateOfBirthIn">
              </div>
              <div class="form-group">
                <label for="EmailIn">Email address</label>
                <input type="email" class="form-control" id="EmailIn" placeholder="Enter Email">
              </div>
              <div class="form-group">
                <label for="PasswordIn">Password</label>
                <input type="password" class="form-control" id="PasswordIn" placeholder="Enter Password">
              </div>
              <div class="form-group">
                <label for="ConfirmPasswordIn">Confirm Password</label>
                <input type="password" class="form-control" id="ConfirmPasswordIn" placeholder="Confirm Password">
              </div>
              <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
        <div class="col"></div>
    </div>
</div>

